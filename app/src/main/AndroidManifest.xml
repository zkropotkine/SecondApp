        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.zkropotkine.secondapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.zkropotkine.secondapp.AddCompany"
            android:label="@string/title_activity_add_company" >
        </activity>
        <activity
            android:name="com.example.zkropotkine.secondapp.SeeCompanies"
            android:label="@string/title_activity_see_companies" >
        </activity>
    </application>

</manifest>
